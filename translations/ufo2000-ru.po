msgstr ""

#: src/text.cpp:502
msgid "Control-panel:  Buttons in column 5:"
msgstr ""

#: src/text.cpp:503
msgid "Switch to next man, who still has TUs left.      "
msgstr ""

#: src/text.cpp:504
msgid "Switch to next man, and mark current man as done."
msgstr ""

